{
  "name": "@lacqueristas/www",
  "version": "1.1.0",
  "description": "The frontend browser client for the lacqueristas API",
  "keywords": [
    "lacqueristas",
    "nailpolish",
    "frontend"
  ],
  "author": "Kurtis Rainbolt-Greene (@krainboltgreene) <me@kurtisrainboltgreene.name>",
  "license": "MIT",
  "homepage": "https://github.com/lacqueristas/www#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lacqueristas/www.git"
  },
  "bugs": {
    "url": "https://github.com/lacqueristas/www/issues"
  },
  "scripts": {
    "start": "node './transpiled/server/index.js'",
    "build:server": "gulp build:server",
    "build:client": "gulp build:client",
    "build": "gulp build:all",
    "watch:lint": "chokidar './source/' --ignore '/node_modules/' --command 'npm run lint --silent'",
    "watch:test": "chokidar './source/' --ignore '/node_modules/' --command 'npm run test --silent'",
    "watch:build": "gulp watch:all",
    "watch": "npm-run-all --parallel watch:build watch:lint watch:test",
    "test": "tap './source/**/test.js'",
    "lint": "eslint './source/' --ignore-pattern 'node_modules'"
  },
  "devDependencies": {
    "babel-eslint": "7.2.3",
    "chokidar-cli": "1.2.0",
    "eslint": "3.19.0",
    "eslint-find-new-rules": "2.0.2",
    "eslint-plugin-fp": "2.3.0",
    "eslint-plugin-promise": "3.5.0"
  },
  "dependencies": {
    "@cycle/dom": "17.4.0",
    "@cycle/run": "3.1.0",
    "@unction/allobjectp": "1.0.4",
    "@unction/allp": "1.0.1",
    "@unction/applicators": "1.0.0",
    "@unction/ispopulated": "2.0.1",
    "@unction/ispresent": "1.1.0",
    "@unction/mapvalues": "2.0.0",
    "@unction/mergedeepright": "1.1.0",
    "@unction/nestedapply": "2.1.2",
    "@unction/nestedobjof": "1.1.0",
    "@unction/reducewithvaluekey": "2.0.0",
    "@unction/rejectp": "1.0.1",
    "@unction/resolvep": "1.0.1",
    "@unction/sample": "1.0.2",
    "@unction/samplesize": "1.0.1",
    "@unction/treeify": "1.1.0",
    "@unction/upto": "2.0.1",
    "@unction/withoutkeyrecursive": "1.1.2",
    "abstraction": "10.3.0",
    "babel-cli": "6.24.1",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-plugin-module-resolver": "2.7.1",
