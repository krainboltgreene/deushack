    "cors": "2.8.3",
    "envify": "4.0.0",
    "express": "4.15.3",
    "font-awesome": "4.7.0",
    "gulp": "3.9.1",
    "gulp-babel": "6.1.2",
    "gulp-changed": "3.1.0",
